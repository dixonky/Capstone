<h1>Edit User Profile Page</h1>

<br>
<h2>Edit User</h2>
<table id="userTab" class="userTable">
    <tr>
        <th>User Name</th>
        <th>User Password</th>
        <th>User Email</th>
    </tr>
    {{#each user}}
        <tr>
            <td>{{user_name}}</td>
             <td>{{user_password}}</td>
             <td>{{user_email}}</td>
            <td>
                <button onclick="" type="button">Update</button>
            </td>
        </tr>
    {{/each}}
</table>

